#include <iostream>
using namespace std;

int main ()
{
	cout<<"* * * * * * * * "<<endl;
	cout<<" * * * * * * * *"<<endl;
	cout<<"* * * * * * * * "<<endl;
	cout<<" * * * * * * * *"<<endl;
	cout<<"* * * * * * * * "<<endl;
	cout<<" * * * * * * * *"<<endl;
	cout<<"* * * * * * * * "<<endl;
	cout<<" * * * * * * * *"<<endl;

return 0;
}

#include <iostream>
using namespace std;

int main ()
{
	cout<<"* * * * * * * *\n  * * * * * * * *\n* * * * * * * *\n  * * * * * * * *\n* * * * * * * *\n  * * * * * * * *\n* * * * * * * *\n  * * * * * * * *\n";
}
	
